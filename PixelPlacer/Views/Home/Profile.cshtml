
@{
    ViewData["Title"] = "Home Page";
}

<h2>My Profile</h2>
<hr />

<div class="container">

    <div class="row">
        <div class="col-md-3 col-sm-3 col-xs-6">
            <a asp-controller="Videos" asp-action="Upload" class="btn btn-default btn-lg btn-block button-4">Upload Video</a>
        </div>
        <div class="col-md-3 col-sm-3 col-xs-6"> 
            <a asp-controller="Videos" asp-action="Index" class="btn btn-default btn-lg btn-block button-3">My Uploads</a>
        </div>
        <div class="col-md-3 col-sm-3 col-xs-6"> 
            <a asp-controller="Projects" asp-action="NewProjectDisplay" class="btn btn-default btn-lg btn-block button-2">Create New Project</a>
        </div>
        <div class="col-md-3 col-sm-3 col-xs-6"> 
            <a asp-controller="Projects" asp-action="Index" class="btn btn-lg btn-block button-1">My Projects</a> 
        </div>
    </div>
</div>
