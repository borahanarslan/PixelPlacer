
@{
    ViewData["Title"] = "Home Page";
}
<div id="Profile-Container">
    <h2>My Profile</h2>
    <hr />

    <div class="container">

        <div class="row">
            <div class="col-md-offset-3 col-md-6 col-sm-6 col-xs-6">
                <a asp-controller="Videos" asp-action="Upload" class="btn btn-default btn-lg btn-block button-1">Upload Video</a>
            </div>
            <br /><br />
            <div class="col-md-offset-3 col-md-6 col-sm-6 col-xs-6">
                <a asp-controller="Videos" asp-action="Index" class="btn btn-default btn-lg btn-block button-2">My Uploads</a>
            </div>
            <br /><br />
            <div class="col-md-offset-3 col-md-6 col-sm-6 col-xs-6">
                <a asp-controller="Projects" asp-action="NewProjectDisplay" class="btn btn-default btn-lg btn-block button-3">Create New Project</a>
            </div>
            <br /><br />
            <div class="col-md-offset-3 col-md-6 col-sm-6 col-xs-6">
                <a asp-controller="Projects" asp-action="Index" class="btn btn-lg btn-block button-4">My Projects</a>
            </div>
            <br /><br />
        </div>
    </div>
</div>