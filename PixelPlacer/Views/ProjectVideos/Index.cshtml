@model IEnumerable<PixelPlacer.Models.ProjectVideos>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.SavedProjectId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.XPositition)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.YPosition)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Video)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SavedProjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.XPositition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YPosition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Video.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProjectVideosId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ProjectVideosId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ProjectVideosId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
